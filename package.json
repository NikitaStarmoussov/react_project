    "unit": "jest --config ./config/jest/jest.config.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown",
      "jest --config ./config/jest/jest.config.ts"
    ],
    "*.{json,html}": [
      "prettier --write --ignore-unknown"
    ],
    "*.scss": [
      "stylelint ./src/**/*.scss --fix"
    ]
  },
